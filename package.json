{
  "name": "rollup-plugin-commonjs",
  "version": "8.2.1",
  "description": "Convert CommonJS modules to ES2015",
  "main": "dist/rollup-plugin-commonjs.cjs.js",
  "module": "dist/rollup-plugin-commonjs.es.js",
  "jsnext:main": "dist/rollup-plugin-commonjs.es.js",
  "scripts": {
    "test": "mocha",
    "pretest": "npm run build",
    "build": "shx rm -rf dist/* && rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/*.js test/*.js",
    "prepare": "npm run lint && npm test",
    "publish-felixrabe": "gitpkg publish"
  },
  "gitpkg": {
    "registry": "https://github.com/felixrabe/rollup-plugin-commonjs.git"
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "dependencies": {
    "acorn": "^5.1.1",
    "estree-walker": "^0.5.0",
    "magic-string": "^0.22.4",
    "resolve": "^1.4.0",
    "rollup-pluginutils": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "gitpkg": "^1.0.0-beta.1",
    "locate-character": "^2.0.1",
    "mocha": "^3.5.0",
    "require-relative": "^0.8.7",
    "rollup": "^0.48.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "shx": "^0.2.2",
    "source-map": "^0.5.7",
    "source-map-support": "^0.4.16"
  },
  "repository": "rollup/rollup-plugin-commonjs",
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rollup/rollup-plugin-commonjs/issues"
  },
  "homepage": "https://github.com/rollup/rollup-plugin-commonjs"
}
